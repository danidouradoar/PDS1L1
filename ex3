#include <stdio.h>

int main(){

    int x;
    int y;
    int z;

    printf("insira 3 numeros inteiros: ");
    scanf("%d %d %d", &x, &y, &z);

    if (x % y == 0 && x % z == 0){
        printf("o numero eh divisivel");
    }
    else{
        printf("o numero nao eh divisivel");
    }

    return 0;
}
