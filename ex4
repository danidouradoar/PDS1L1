#include <stdio.h>

int main(){

    int valor;
    int n100;
    int n50;
    int n20;
    int n10;
    int n5;
    int n2;
    int n1;

    printf("insira um valor inteiro em reais: ");
    scanf("%d", &valor);
    
    n100 = valor/100;
        printf("100: %d\n", n100);
    valor = valor%100;
    n50 = (valor/50);
        printf("50: %d\n", n50);
    valor = valor%50;
    n20 = (valor/20);
        printf("20: %d\n", n20);
    valor = valor%20;
    n10 = (valor/10);
        printf("10: %d\n", n10);
    valor = valor%10;
    n5 = (valor/5);
        printf("5: %d\n", n5);
    valor = valor%5;
    n2 = (valor/2);
        printf("2: %d\n", n2);
    n1 = valor%2;
        printf("1: %d\n", n1);
    return 0;
}
